	<queryString><![CDATA[SELECT DISTINCT emp.nombre AS NomEmpresa, emp.direccion AS EmpDire, emp.municipio AS EmpMuni, emp.departamento AS EmpDepa, emp.nit AS EmpNit, emp.telefono AS EmpTel, emp.logo AS EmpLogo, encf.*, DATE_FORMAT(NOW(), '%d/%m/%Y %h:%i:%s%p') AS FechaHora, 
su.usuario, detf.clase_servicio, SUM(detf.cantidad) AS cantidad, SUM(detf.cantidad*detf.valor) AS valorSum, 
enti.modalidad_pago,fnum.consecutivo
FROM empresa emp, fact_enc_factura encf, fact_det_factura detf, seg_usuario su, adm_entidad enti,fact_numeradas fnum
WHERE fnum.consecutivo=$P{consecutivo} AND fnum.cod_enc_fact_fk=encf.cod_enc_factura AND encf.cod_enc_factura=detf.cod_enc_factura_fk 
AND enti.ent_nit=encf.cod_eps and fnum.estadoFact!=5 AND su.usu_codigo=encf.cod_usuario_fk
GROUP BY detf.tipopop LIMIT 1;]]></queryString>

