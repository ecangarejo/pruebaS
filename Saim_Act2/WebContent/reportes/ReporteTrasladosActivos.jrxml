	<queryString><![CDATA[SELECT CONCAT(apac.nombre,' ',apac.primer_apellido,' ',apac.segundo_apellido) as Paciente,
CONCAT(aad.fecha_registro,'/',aad.hora_registro) as FechaAdmision,
CONCAT(acc.ubicacion_cama,'>>',acc.codigocama) as Ubicacion,
CONCAT(sdt.nombre,' ',sdt.apellido) as Usuario_Traslado, 
CONCAT(ahc.fecha_inicial,'/',ahc.hora_inicial) as Fecha_Hora_Traslado,emp.nombre AS NomEmpresa,emp.direccion AS EmpDire,emp.municipio AS EmpMuni,emp.departamento AS EmpDepa,emp.nit AS EmpNit,emp.telefono AS EmpTel,emp.logo AS EmpLogo
FROM adm_admisiones aad,adm_historico_cama ahc,seg_datos_personales sdt,
seg_usuario su,adm_censo_cama acc,adm_paciente apac,empresa emp
WHERE aad.adm_numero_ingreso=ahc.adm_numero_ingreso_fk 
and ahc.fecha_final='0000-00-00' and ahc.hora_final='00:00:00' and ahc.usuarioFin='-1' 
and sdt.dat_codigo=su.dat_codigo_fk 
and su.usu_codigo=ahc.usuarioIni 
and acc.cen_numero_cama=ahc.cen_numero_cama_fk 
and aad.estado=0 
and apac.pac_codigo_paciente=aad.pac_codigo_paciente_fk 
and acc.ubicacion_cama=$P{servicio}]]></queryString>

