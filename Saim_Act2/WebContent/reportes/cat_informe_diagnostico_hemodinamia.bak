	<queryString><![CDATA[SELECT
CONCAT(pac.`tipo_documento`, " ",pac.`numero_documento`) AS 'Identificacion' ,
CONCAT(pac.`nombre`, " ", pac.`primer_apellido`," ",pac.`segundo_apellido`) AS 'NombrePaciente',
(YEAR(CURDATE())-YEAR(pac.fecha_nacimiento))- (RIGHT(CURDATE(),5)<RIGHT(pac.fecha_nacimiento,5))AS 'Edad',
pac.genero AS 'Sexo',
ent.nombre_entidad AS 'NombreEntidad',
CONCAT(inf.fecha_diagnostico," ",inf.hora_diagnostico) AS 'fecha',
est.nombre_estudio_realizado AS "estudio realizado" ,
ind.nombre_indicacion_diagnostico_hemodinamia AS "indicacion",
inf.nombre_Anesteciologo AS "Anestesiologo",
cont1.contenido_informe_ventriculografia AS 'ventriculografia',
cont1.contenido_informe_aortograma AS 'aortograma',
cont1.resumen_procedimiento AS 'informe',
cont1.conclusiones AS 'conclusiones',
cont1.recomendaciones AS 'recomendaciones',
cont1.imagen_anatomia_coronaria AS 'imagenAnatomiaCoronaria',
cont1.imagen_ventriculografia AS 'imagenVentriculografia',
cont2.descripcion_arteria_TCI AS 'TCI',
cont2.descripcion_arteria_ADA AS 'ADA',
cont2.descripcion_arteria_AC AS 'AC',
cont2.descripcion_arteria_ACD AS 'ACD',
datos.firma AS'FirmaMedico',
datos.firma AS'FirmaMedico2',
CONCAT(datos.nombre," ",datos.apellido) AS 'NombreMedico',
datos.ocupacion AS 'OcupacionMedico',
datos.cedula AS 'CedulaMedico',
emp.nombre AS 'NombreEmpresa',
emp.direccion AS 'DireccionEmpresa',
CONCAT(emp.municipio , " - ",emp.departamento) AS 'DepartamentoCiudad',
emp.nit AS 'NitEmpresa',
emp.telefono AS 'TelefonoEmpresa',
emp.logo AS 'LogoEmpresa',
emp.incontec AS 'IncontecEmpresa'
FROM
`adm_paciente` pac,
`cat_informe_diagnostico_hemodinamia` inf,
`cat_estudio_realizado_hemodinamia` est,
`cat_indicacion_diagnostico_hemodinamia` ind,
`cat_contenido_informe_hemodinamia` cont1,
`cat_contenido_informe_anatomia_coronaria` cont2, 
`adm_convenio` con,
`adm_entidad` ent,
`seg_usuario` seg,
`seg_datos_personales` datos,
`empresa` emp
WHERE
pac.`pac_codigo_paciente` = inf.id_paciente
AND inf.id_informe_diagnostico_hemodinamia = $P{CodInfCateterismo} 
AND inf.id_informe_diagnostico_hemodinamia = cont1.id_informe_diagnostico_hemodinamia_fk
AND inf.id_informe_diagnostico_hemodinamia = cont2.id_informe_diagnostico_hemodinamia_fk
AND est.id_estudio_realizado_hemodinamia = inf.id_estudio_realizado_hemodinamia_fk 
AND ind.id_tipo_indicacion_diagnostico_hemodinamia = inf.id_tipo_indicacion_diagnostico_hemodinamia_fk 
AND ent.ent_nit=con.ent_nit_contratante_fk
AND pac.conv_numero_contrato_fk=con.conv_numero_contrato 
AND inf.id_usuario = seg.usu_codigo
AND seg.dat_codigo_fk = datos.dat_codigo]]></queryString>

