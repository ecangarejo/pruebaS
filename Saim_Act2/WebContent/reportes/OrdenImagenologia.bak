	<parameter name="CodOrden" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT hdi.codigo AS CodDetalle,isub.nombre,hdi.observacion AS ObsDetOrd,hod.observacion AS ObsOrden,
ap.nombre AS NomPac,ap.primer_apellido,ap.segundo_apellido,
CONCAT(ap.tipo_documento,'-',ap.numero_documento) AS Documento,ap.genero,
(YEAR(CURDATE())-YEAR(ap.fecha_nacimiento)) - (RIGHT(CURDATE(),5) < RIGHT(ap.fecha_nacimiento, 5)) AS Edad,
aen.nombre_entidad,CONCAT(aad.fecha_registro,'/',aad.hora_registro) AS FechaRegistro,
acc.pabellon,acc.ubicacion_cama,acc.codigocama,CONCAT(sdt.nombre,' ',sdt.apellido) AS Responsable,
sdt.profesion,sdt.cedula AS TarjetaProfesional,sdt.firma,emp.nombre AS NomEmpresa,emp.direccion AS EmpDire,emp.municipio AS EmpMuni,emp.departamento AS EmpDepa,emp.nit AS EmpNit,emp.telefono AS EmpTel,emp.logo AS EmpLogo,CONCAT(hod.fecha,'/',hod.hora) as FechaOrden  
FROM hic_orden hod,adm_paciente ap,img_subexa isub,
hic_detalleordenimagenes hdi,adm_admisiones aad,adm_convenio acv,adm_entidad aen,
adm_censo_cama acc,seg_usuario sgu,seg_datos_personales sdt,empresa emp 
WHERE ap.pac_codigo_paciente=hod.CodPac_fk 
AND acc.cen_numero_cama=aad.cen_numero_cama_fk 
AND isub.codigo=hdi.CodEstIma_fk 
AND hod.codigo=hdi.CodOrdImg_fk 
AND aad.adm_numero_ingreso=hod.CodAdm_fk 
AND aen.ent_nit=acv.ent_nit_contratante_fk 
AND acv.conv_numero_contrato=ap.conv_numero_contrato_fk 
AND hod.CodUsu_fk=sgu.usu_codigo 
AND sdt.dat_codigo=sgu.dat_codigo_fk
AND hod.codigo=$P{CodOrden}]]></queryString>

