	<queryString><![CDATA[SELECT CONCAT(p.nombre,' ',p.primer_apellido,' ',p.segundo_apellido) AS Paciente,CONCAT(c.pabellon,'>>',c.ubicacion_cama,'>>',c.codigocama ) as Ubicacion,ent.nombre_entidad,
NOW() AS hora,
 emp.nombre AS NomEmpresa,
 emp.nit AS EmpNit,emp.logo AS EmpLogo
FROM  adm_paciente p, adm_censo_cama c, adm_admisiones a, adm_entidad ent,adm_convenio con,empresa emp
WHERE a.pac_codigo_paciente_fk = p.pac_codigo_paciente 
AND c.cen_numero_cama = a.cen_numero_cama_fk 
AND ent.ent_nit = con.ent_nit_contratante_fk  
AND con.conv_numero_contrato = p.conv_numero_contrato_fk 
AND a.estado='0' 
AND c.servicio > 1
group by p.pac_codigo_paciente ORDER BY p.nombre]]></queryString>

