	<queryString><![CDATA[SELECT NOW() AS hora,emp.nombre AS NomEmpresa,emp.nit AS EmpNit,emp.logo AS EmpLogo,(YEAR(CURDATE())-YEAR(p.fecha_nacimiento)) - (RIGHT(CURDATE(),5) < RIGHT(p.fecha_nacimiento, 5)) AS age , 
DATEDIFF(CURDATE(),a.fecha_registro) AS Dias,c.codigocama, c.pabellon,
 a.semana_cotizadas, a.fecha_registro, p.genero, CONCAT(p.nombre,' ',p.primer_apellido,' ',p.segundo_apellido) AS Paciente, ent.nombre_entidad, 
 p.pac_codigo_paciente,p.fecha_nacimiento,a.observaciones,c.ubicacion_cama,hdi.codDiagnostico
FROM  adm_paciente p, adm_censo_cama c, adm_admisiones a, adm_entidad ent, adm_convenio con,empresa emp,hic_diagnosticoingreso hdi   
WHERE a.pac_codigo_paciente_fk = p.pac_codigo_paciente AND 
c.cen_numero_cama = a.cen_numero_cama_fk AND 
ent.ent_nit = con.ent_nit_contratante_fk AND 
con.conv_numero_contrato = p.conv_numero_contrato_fk AND a.estado='0'
AND c.ubicacion_cama=$P{NomSubArea} 
AND hdi.codAdm=a.adm_numero_ingreso 
group by p.pac_codigo_paciente ORDER BY c.ubicacion_cama]]></queryString>

