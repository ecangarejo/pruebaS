	<parameter name="CodFormulacion" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT med.codigoMedicamento,CONCAT(med.nombre,'/',med.concentracion) AS medicamento,hdfr.dosis,hdfr.observacion,hdfr.cantidad,CONCAT(aad.fecha_registro,'/',aad.hora_registro) AS Ingreso,
CONCAT(apac.nombre,' ',apac.primer_apellido,' ',apac.segundo_apellido) AS Paciente,CONCAT(apac.tipo_documento,'-',apac.numero_documento) AS identificacion,(YEAR(CURDATE())-YEAR(apac.fecha_nacimiento)) - (RIGHT(CURDATE(),5) < RIGHT(apac.fecha_nacimiento, 5)) AS Edad,apac.genero,
aen.nombre_entidad,aar.nombre AS AreaUbicacion,asub.nombre AS NomSubarea,acc.codigocama AS cama,CONCAT(hfr.hora,'/',hfr.fecha) as HoraFechaFormu,hfr.observacion as OrdenMedica,
CONCAT(sdt.nombre,' ',sdt.apellido) AS Responsable,sdt.profesion,sdt.cedula AS TarjetaProfesional,sdt.firma,emp.nombre AS NomEmpresa,emp.direccion AS EmpDire,emp.municipio AS EmpMuni,emp.departamento AS EmpDepa,emp.nit AS EmpNit,emp.telefono AS EmpTel,emp.logo AS EmpLogo  
FROM hic_formulacion hfr,hic_detalle_formulacion hdfr,medicamentos med,adm_admisiones aad,
adm_paciente apac,adm_convenio acv,adm_entidad aen,adm_area aar,adm_subarea asub,adm_censo_cama acc,
seg_usuario sgu,seg_datos_personales sdt,empresa emp
WHERE hfr.codigo=hdfr.CodFormulacion_fk 
AND med.codigo=hdfr.CodMedicamento_fk 
AND hfr.codigo=$P{CodFormulacion} 
AND aad.adm_numero_ingreso=hfr.CodAdm_fk 
AND apac.pac_codigo_paciente=hfr.CodPac_fk 
AND apac.pac_codigo_paciente=aad.pac_codigo_paciente_fk 
AND aen.ent_nit=acv.ent_nit_contratante_fk 
AND acv.conv_numero_contrato=apac.conv_numero_contrato_fk 
AND aar.codigo=hfr.CodArea_fk 
AND asub.codigo=hfr.CodSubarea_fk 
AND acc.cen_numero_cama=hfr.CodCama_fk 
AND hfr.CodUsu_fk=sgu.usu_codigo 
AND sdt.dat_codigo=sgu.dat_codigo_fk]]></queryString>

