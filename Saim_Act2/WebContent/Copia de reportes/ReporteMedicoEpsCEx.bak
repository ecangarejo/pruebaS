	<queryString><![CDATA[SELECT su.usu_codigo,emp.nombre AS NomEmpresa,emp.direccion AS EmpDire,emp.municipio AS EmpMuni,emp.departamento AS EmpDepa,emp.nit AS EmpNit,emp.telefono AS EmpTel,emp.logo AS EmpLogo
FROM agm_citaspacientes acp,agm_horariomedico ahm,seg_usuario su,seg_datos_personales sdt,agm_medico amed,empresa emp
WHERE acp.CodHorMedico_fk=ahm.codigo 
AND acp.CodMedico_fk=amed.codigo 
AND amed.numeroDocumento=sdt.numeroDocumento 
AND sdt.dat_codigo=su.dat_codigo_fk
AND ahm.estado=3 
AND acp.fecha BETWEEN $P{FechaIni} AND $P{FechaFin}
GROUP BY su.usu_codigo]]></queryString>

