	<queryString><![CDATA[SELECT acv.conv_numero_contrato,emp.nombre AS NomEmpresa,emp.direccion AS EmpDire,emp.municipio AS EmpMuni,emp.departamento AS EmpDepa,emp.nit AS EmpNit,emp.telefono AS EmpTel,emp.logo AS EmpLogo
FROM agm_citaspacientes acp,agm_horariomedico ahm,empresa emp,adm_paciente pac,adm_entidad aen,adm_convenio acv
WHERE acp.CodHorMedico_fk=ahm.codigo 
AND pac.pac_codigo_paciente=acp.CodPac_fk 
AND pac.conv_numero_contrato_fk=acv.conv_numero_contrato 
AND aen.ent_nit=acv.ent_nit_contratante_fk 
AND ahm.estado=4 
AND acp.fecha BETWEEN $P{FechaIni} AND $P{FechaFin}  
GROUP BY aen.nombre_entidad ]]></queryString>

