	<parameter name="fechaIni" isForPrompting="false" class="java.lang.String"/>
	<parameter name="fechaFin" isForPrompting="false" class="java.lang.String"/>
	<parameter name="CodigoConv" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select count(apac.pac_codigo_paciente) as cont,ent.nombre_entidad,aesp.nombre_especialidad
from agm_citaspacientes acp,adm_paciente apac,adm_entidad ent,adm_convenio con,agm_especialidad aesp
where acp.CodPac_fk=apac.pac_codigo_paciente
and apac.conv_numero_contrato_fk=con.conv_numero_contrato 
and ent.ent_nit=con.ent_nit_contratante_fk 
and aesp.codigo=acp.CodEspecialidad_fk
and apac.conv_numero_contrato_fk=$P{CodigoConv}
and acp.fecha between $P{fechaIni} and $P{fechaFin}
group by(acp.CodEspecialidad_fk)]]></queryString>

	<field name="cont" class="java.lang.Integer"/>
	<field name="nombre_entidad" class="java.lang.String"/>
	<field name="nombre_especialidad" class="java.lang.String"/>

