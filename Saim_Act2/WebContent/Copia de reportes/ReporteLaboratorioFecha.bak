	<parameter name="fechaIni" isForPrompting="false" class="java.lang.String"/>
	<parameter name="fechaFin" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select * from (select count(opl.codigo_estudio) as cont,lsub.nombre,opl.codigo_estudio,emp.nombre as NomEmpresa,emp.direccion as EmpDire,emp.municipio as EmpMuni,emp.departamento as EmpDepa,emp.nit as EmpNit,emp.telefono as EmpTel,emp.logo as EmpLogo
from lab_area lar,lab_subarea lsub,ord_pac_lab opl,empresa emp
where lar.codigo=lsub.codarea_fk 
and lsub.codigo=opl.codigo_estudio
and opl.fecha between $P{fechaIni} and $P{fechaFin}
group by lsub.nombre 
union
select count(opl.codigo_estudio) as cont,lex.nombre,opl.codigo_estudio,emp.nombre as NomEmpresa,emp.direccion as EmpDire,emp.municipio as EmpMuni,emp.departamento as EmpDepa,emp.nit as EmpNit,emp.telefono as EmpTel,emp.logo as EmpLogo
from ord_pac_lab opl,lab_examen lex,empresa emp
where lex.codigo=opl.codigo_estudio
and opl.fecha between $P{fechaIni} and $P{fechaFin}
group by lex.nombre) as c order by c.cont desc ]]></queryString>

